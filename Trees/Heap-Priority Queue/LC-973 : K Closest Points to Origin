#LEETCODE 973
import math
class Solution:
    def kClosest(self, points: List[List[int]], k: int) -> List[List[int]]:
        heap = []
        for i in points:
            dis = math.sqrt(i[0]**2+i[1]**2)
            heapq.heappush(heap, (dis, i))
        res = []
        for _ in range(k):
            value, key = heapq.heappop(heap)
            res.append(key)
        return res
